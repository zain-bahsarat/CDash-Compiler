#MODE 10
declaration
start
    output "Enter operand 1"
    input i
    output "Enter operand 2"
    input j
    output "Enter operator (+,-,*,/)"
    input ch
    if(a>11)
    output "if condition"
    else
    output "output condition"
    endif
switch(ch)
    cases
    '+';result=i+j break
    '+';result=i+j break
    else;output "Wrong Operator" break
    endcase
for a=1,10,+1
